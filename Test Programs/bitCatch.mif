-- Altera Memory Initialization File (MIF)

DEPTH = 2048;
WIDTH = 16;

ADDRESS_RADIX = HEX;
DATA_RADIX = HEX;

CONTENT
  BEGIN
    000 : b800; -- LOADI  0
    001 : 1023; -- STORE  Score
    002 : 9804; -- OUT    Hex0
    003 : ba00; -- LOADI  &B1000000000
    004 : 1022; -- STORE  Pattern
    005 : 9801; -- OUT    LEDs
    006 : 9000; -- IN     Switches
    007 : 4824; -- AND    Bit0
    008 : 3812; -- JPOS   CheckCatch   ; SW up, check if LED caught
    009 : 0822; -- LOAD   Pattern
    00a : 6011; -- SHIFT  -1
    00b : 1022; -- STORE  Pattern
    00c : 9801; -- OUT    LEDs
    00d : 381d; -- JPOS   Delay        ; Not fallen off; go to delay
    00e : ba00; -- LOADI  &B1000000000
    00f : 1022; -- STORE  Pattern
    010 : 9801; -- OUT    LEDs
    011 : 281d; -- JUMP   Delay
    012 : 0822; -- LOAD   Pattern
    013 : 4824; -- AND    Bit0
    014 : 4019; -- JZERO  WaitForResume  ; miss, no points
    015 : 0823; -- LOAD   Score
    016 : 6801; -- ADDI   1
    017 : 1023; -- STORE  Score
    018 : 9804; -- OUT    Hex0
    019 : 9000; -- IN     Switches
    01a : 4824; -- AND    Bit0
    01b : 3819; -- JPOS   WaitForResume
    01c : 2809; -- JUMP   ShiftLEDs
    01d : 9802; -- OUT    Timer
    01e : 9002; -- IN     Timer
    01f : 6fff; -- ADDI   -1
    020 : 301e; -- JNEG   WaitingLoop
    021 : 2806; -- JUMP   CheckSW0
    022 : 0000; -- Pattern:   DW 0
    023 : 0000; -- Score:     DW 0
    024 : 0001; -- Bit0:      DW &B0000000001

    [025..7ff] : 0000; -- Empty memory

  END;
